<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_praise"
    android:layout_width="match_parent"
    android:background="#f1f1f1"
    android:layout_height="match_parent"
    tools:context="com.technology.yuyidoctorpad.activity.MyPraise.MyPraiseActivity">
    <include layout="@layout/titleinclude"
        android:id="@+id/myPraiseInclude"/>
    <RelativeLayout
        android:layout_below="@+id/myPraiseInclude"
        style="@style/LayoutMatch">
        <com.technology.yuyidoctorpad.lzhUtils.MyListView
            android:scrollbars="none"
            android:layout_marginTop="10dp"
            android:layout_marginRight="100dp"
            android:layout_marginLeft="100dp"
            android:dividerHeight="10dp"
            android:divider="#00000000"
            android:id="@+id/myPraiseListview"
            android:listSelector="#00ffffff"
            style="@style/LayoutMatch"/>
        </RelativeLayout>
</RelativeLayout>
