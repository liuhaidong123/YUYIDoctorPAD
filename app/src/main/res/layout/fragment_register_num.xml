    <RelativeLayout
        android:paddingRight="50dp"
        android:paddingLeft="45dp"
        android:background="@color/white"
        android:layout_marginTop="1dp"
        android:layout_width="match_parent"
        android:layout_height="60dp">
        <ImageView
            android:id="@+id/registion_imgAllSelect"
            android:layout_centerVertical="true"
            android:padding="5dp"
            android:src="@drawable/imgse"
            style="@style/LayoutWrap"/>
        <TextView
            android:id="@+id/text1"
            android:layout_marginLeft="5dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/registion_imgAllSelect"
            style="@style/TextS14C3Lwrap"
            android:text="全选"/>
        <LinearLayout
            android:layout_marginLeft="50dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/text1">
            <!--科室-->
            <TextView  style="@style/TextS14C3Lwrap"
            android:text="科室"
            android:id="@+id/text2"
            android:layout_centerVertical="true"
            />
            <RelativeLayout
                android:paddingLeft="5dp"
                android:paddingRight="10dp"
                android:id="@+id/registion_layoutKS"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/text2"
                android:layout_marginLeft="10dp"
                android:background="@drawable/layout"
                android:layout_width="150dp"
                android:layout_height="30dp"
                >
                <TextView
                    android:imeOptions="actionSearch"
                    android:id="@+id/registion_KSName"
                    android:layout_centerVertical="true"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textSize="@dimen/size12"
                    android:textColor="@color/color_333333"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="全部"/>
                <ImageView
                    android:id="@+id/registion_KSimg"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="15dp"
                    android:layout_height="10dp"
                    android:background="@drawable/imgselect"/>
            </RelativeLayout>
            <!--时间-->
            <TextView
                android:layout_marginLeft="50dp"
                style="@style/TextS14C3Lwrap"
                android:text="时间"
                android:id="@+id/text3"
                android:layout_centerVertical="true"
                />
            <RelativeLayout
                android:paddingLeft="5dp"
                android:paddingRight="10dp"
                android:id="@+id/registion_layoutTime"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@+id/text2"
                android:layout_marginLeft="10dp"
                android:background="@drawable/layout"
                android:layout_width="150dp"
                android:layout_height="30dp"
                >
                <TextView
                    android:id="@+id/registion_TimeText"
                    android:layout_centerVertical="true"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textSize="@dimen/size12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#333333"
                    android:text="11月20日-上午"/>
                <ImageView
                    android:id="@+id/registion_Timeimg"
                    android:layout_centerVertical="true"
                    android:layout_alignParentRight="true"
                    android:layout_width="15dp"
                    android:layout_height="10dp"
                    android:background="@drawable/imgselect"/>
            </RelativeLayout>
            <!--挂号数-->
            <TextView
                android:layout_marginLeft="50dp"
                style="@style/TextS14C3Lwrap"
                android:text="设置挂号数"
                android:id="@+id/text4"
                android:layout_centerVertical="true"
                />
            <EditText
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:id="@+id/registion_RegisterNum"
                android:inputType="number"
                android:maxLength="2"
                android:layout_marginLeft="10dp"
                android:background="@drawable/layout"
                android:textColor="@color/color_333333"
                android:textSize="14sp"
                android:layout_width="60dp"
                android:layout_height="30dp"/>
        </LinearLayout>
            <TextView
                android:id="@+id/registion_submitT"
                android:gravity="center"
                android:textColor="#FFffff"
                android:textSize="14sp"
                android:background="@drawable/btn"
                android:text="确定"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_width="71dp"
                android:layout_height="30dp" />
        </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="1dp"
        android:background="@color/white">
        <com.technology.yuyidoctorpad.lzhUtils.MyListView
            android:layout_width="match_parent"
            android:dividerHeight="1dp"
            android:divider="#e5e5e5"
            android:id="@+id/registion_listView"
            android:layout_height="match_parent">


        </com.technology.yuyidoctorpad.lzhUtils.MyListView>
    </RelativeLayout>
</LinearLayout>
