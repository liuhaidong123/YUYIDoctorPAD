<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_centerHorizontal="true"
        android:background="@color/white"
        android:layout_width="673dp"
        android:layout_height="128dp">
        <com.technology.yuyidoctorpad.lhdUtils.RoundImageView
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:src="@mipmap/erroruser"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="20dp"
            android:layout_centerVertical="true"
            android:id="@+id/my_patientList_image"
            />
        <TextView
            android:text=""
            android:textColor="#333333"
            android:textSize="19sp"
            android:layout_toRightOf="@+id/my_patientList_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:id="@+id/my_patientList_textVname"/>
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="25dp"
            android:src="@mipmap/many"
            android:layout_alignParentRight="true"/>
    </RelativeLayout>

</RelativeLayout>